  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A5ADBA"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#36B37E"
      }
    },
    {
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "#FF8B00"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#8777D9"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#4C9AFF"
      }
    },
    {
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#FF8B00"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#8777D9"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#4C9AFF"
      }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#A5ADBA"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#FF8B00"
      }
    },
    {
      "scope": ["meta.decorator", "meta.decorator punctuation.decorator"],
      "settings": {
        "foreground": "#4DD0E1"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FF8B00"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#FF5630"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#4C9AFF"
      }
    },
    {
      "scope": ["storage.type.builtin"],
      "settings": {
        "foreground": "#8777D9"
      }
    },
    {
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#4C9AFF"
      }
    },
    {
      "scope": ["meta.block", "meta.object-literal.key"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#FF8B00"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#4DD0E1"
      }
    },
    {
      "scope": ["keyword.operator.new"],
      "settings": {
        "foreground": "#8777D9"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#8777D9",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#36B37E"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list punctuation.definition.list.begin"],
      "settings": {
        "foreground": "#8777D9"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#36B37E"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5630"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#FF8B00"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#A5ADBA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.link"],
      "settings": {
        "foreground": "#4C9AFF"
      }
    }
  ]
}
